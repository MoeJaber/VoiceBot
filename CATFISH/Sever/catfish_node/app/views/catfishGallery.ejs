<!DOCTYPE html>
<html>


<head>
  <% include partials/template/head.ejs %>
    <link rel="stylesheet" href="assets1/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="assets1/css/Dynamically-Queue-Videos.css">
    <link rel="stylesheet" href="assets1/css/Filterable-Gallery.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="assets1/css/styles.css">

  <link rel="stylesheet" href="video_gallery/css/main.css">
  <link href='http://fonts.googleapis.com/css?family=Noto+Sans:400,700' rel='stylesheet' type='text/css'>
   <style>
      html,
      body {
        margin: 0;
        padding: 0;
        height: 100%;
      }

      #header {
        background: #ff0;
        padding: 10px;
      }

      .header-dark {
        padding: 10px;
        padding-bottom: 800px;
        /* Height of the footer */
      }

      #containers {
        min-height: 100%;
        position: relative;
        background-color: #313131;
      }

      .footer-dark {
       /* position: absolute; */
        bottom: 0;
        width: 100%;
        height: 300px;
        /* Height of the footer */
      }
    </style>
</head>

<div class="containers">
  <body>
    <% include partials/template/header.ejs %>

<% videos.forEach(function(item){  %>
     <div class="video">
        <figure>
            <a title="<%=item.videoDesc%>" class="fancybox fancybox.iframe" href="//www.youtube.com/embed/<%=item.videoYoutubeId%>">
              <img class="videoThumb" src="http://i1.ytimg.com/vi/<%=item.videoYoutubeId%>/mqdefault.jpg"/>
            </a>
            <figcaption class="text-center"><%=item.videoTitle%></figcaption>
       </figure>
    </div>
<% }); %>
</div>

  
          <% include partials/template/jsdefaults.ejs %>
<script src="assets/js/jquery.min.js"></script>
    <script src="assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.0/jquery.min.js"></script>
    <script src="video_gallery/js/jquery.fancybox.min.js"></script>
    <script src="video_gallery/js/global.min.js"></script>
     <script>
    $(document).ready(function() {
      $('.fancybox').fancybox({
        padding   : 0,
        maxWidth  : '100%',
        maxHeight : '100%',
        width   : 853,
        height    : 480,
        autoSize  : true,
        closeClick  : true,
        openEffect  : 'elastic',
        closeEffect : 'elastic'
      });
    });
  </script>
</body>
</div>

<% include partials/template/footer.ejs %>
</html>
